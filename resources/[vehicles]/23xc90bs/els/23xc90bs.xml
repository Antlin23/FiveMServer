<?xml version="1.0" encoding="utf-8"?>
<!-- ELS VEHICLE CONFIGURATION FILE -->
<vcfroot Description="[https://www.x-modding.de] Ford S-Max Kripo " Author="Jakob [https://www.x-modding.de]" >
	<!-- INTERFACE -->
	<INTERFACE>
		<LstgActivationType>auto</LstgActivationType>
		<DefaultSirenMode>standby</DefaultSirenMode>
		<InfoPanelHeaderColor>grey</InfoPanelHeaderColor>
		<InfoPanelButtonLightColor>orange</InfoPanelButtonLightColor>
	</INTERFACE>
	<!-- EXTRAS OVERRIDE AND ENV LIGHTING-->
	<EOVERRIDE>	
		<Extra01 IsElsControlled="true" AllowEnvLight="true" Color="orange" OffsetX="-0.70" OffsetY="-0.20" OffsetZ="0.50" />
		<Extra02 IsElsControlled="true" AllowEnvLight="true" Color="orange" OffsetX="-0.25" OffsetY="-0.20" OffsetZ="0.50" />
		<Extra03 IsElsControlled="true" AllowEnvLight="true" Color="orange" OffsetX="0.25" OffsetY="-0.20" OffsetZ="0.50" />
		<Extra04 IsElsControlled="true" AllowEnvLight="true" Color="orange" OffsetX="0.70" OffsetY="-0.20" OffsetZ="0.50" />
		<Extra05 IsElsControlled="true" AllowEnvLight="true" Color="orange" OffsetX="-0.70" OffsetY="-0.20" OffsetZ="0.50" />
		<Extra06 IsElsControlled="true" AllowEnvLight="true" Color="orange" OffsetX="0.70" OffsetY="-0.20" OffsetZ="0.50" />
		<Extra07 IsElsControlled="true" AllowEnvLight="true" Color="orange" OffsetX="-0.40" OffsetY="-0.80" OffsetZ="0.40" />
		<Extra08 IsElsControlled="true" AllowEnvLight="true" Color="orange" OffsetX="0.00" OffsetY="-0.80" OffsetZ="0.40" />
		<Extra09 IsElsControlled="true" AllowEnvLight="true" Color="orange" OffsetX="0.40" OffsetY="-0.80" OffsetZ="0.40" />
		<Extra10 IsElsControlled="true" AllowEnvLight="true" Color="orange" OffsetX="0.40" OffsetY="-0.80" OffsetZ="0.40" />
		<Extra11 IsElsControlled="true" AllowEnvLight="true" Color="orange" OffsetX="0.40" OffsetY="-0.80" OffsetZ="0.40" />
		<Extra12 IsElsControlled="true" AllowEnvLight="true" Color="orange" OffsetX="0.40" OffsetY="-0.80" OffsetZ="0.40" />
	</EOVERRIDE>
	<!-- MISC -->
	<MISC>
		<VehicleIsSlicktop>false</VehicleIsSlicktop>
		<ArrowboardType>off</ArrowboardType>
		<UseSteadyBurnLights>false</UseSteadyBurnLights>
		<DfltSirenLtsActivateAtLstg>3</DfltSirenLtsActivateAtLstg>
		<Takedowns AllowUse="true" Mirrored="false" />
		<SceneLights AllowUse="true" IlluminateSidesOnly="true" />	
	</MISC>
	<!-- CRUISE -->
	<CRUISE>
		<DisableAtLstg3>true</DisableAtLstg3>
		<UseExtras Extra1="false" Extra2="false" Extra3="false" Extra4="false" />
	</CRUISE>
	<!-- AUX CORONAS -->
	<ACORONAS>
		<Headlights DfltPattern="0" ColorL="white" ColorR="white" />
		<TailLights DfltPattern="0" ColorL="red" ColorR="red" />
		<IndicatorsF DfltPattern="0" ColorL="white" ColorR="white" />
		<IndicatorsB DfltPattern="0" ColorL="red" ColorR="red" />
		<ReverseLights DfltPattern="0" ColorL="white" ColorR="white" />
	</ACORONAS>
	<!-- SOUNDS -->
	<SOUNDS>
		<MainHorn InterruptsSiren="true" AudioString="siren_hotel" />
		<ManTone1 AllowUse="true" AudioString="wmsiren/siren_hotel" />
		<ManTone2 AllowUse="true" AudioString="wmsiren/siren_hotel" />
		<SrnTone1 AllowUse="true" AudioString="wmsiren/siren_delta" />
		<SrnTone2 AllowUse="true" AudioString="wmsiren/siren_delta" />
		<SrnTone3 AllowUse="true" AudioString="wmsiren/siren_delta" />
		<SrnTone4 AllowUse="true" AudioString="wmsiren/siren_delta" />
		<AuxSiren AllowUse="true" AudioString="wmsiren/siren_hotel" />
		<PanicMde AllowUse="true" AudioString="wmsiren/siren_hotel" />
	</SOUNDS>
	<!-- WRNL -->
	<WRNL LightingFormat="leds" >
		<PresetPatterns>
			<Lstg3 Enabled="true" Pattern="12" />
		</PresetPatterns>
		<ForcedPatterns>
			<MainHorn Enabled="false" Pattern="12" />
			<SrnTone1 Enabled="false" Pattern="12" />
			<SrnTone2 Enabled="false" Pattern="12" />
			<SrnTone3 Enabled="false" Pattern="12" />
			<SrnTone4 Enabled="false" Pattern="12" />
			<PanicMde Enabled="false" Pattern="12" />
			<OutOfVeh Enabled="false" Pattern="12" />
		</ForcedPatterns>
		<ScanPatternCustomPool Enabled="true" Sequential="true" >
			<Pattern>12</Pattern>
			<Pattern>12</Pattern>
			<Pattern>12</Pattern>
		</ScanPatternCustomPool>
	</WRNL>
	<!-- PRML -->
	<PRML LightingFormat="leds" ExtrasActiveAtLstg2="1and4" >
		<PresetPatterns>
			<Lstg2 Enabled="true" Pattern="60" />
			<Lstg3 Enabled="true" Pattern="60" />
		</PresetPatterns>
		<ForcedPatterns>
			<MainHorn Enabled="false" Pattern="60" />
			<SrnTone1 Enabled="false" Pattern="60" />
			<SrnTone2 Enabled="false" Pattern="60" />
			<SrnTone3 Enabled="false" Pattern="60" />
			<SrnTone4 Enabled="false" Pattern="60" />
			<PanicMde Enabled="false" Pattern="60" />
			<OutOfVeh Enabled="false" Pattern="60" />
		</ForcedPatterns>
		<ScanPatternCustomPool Enabled="true" Sequential="true">
			<Pattern>60</Pattern>
			<Pattern>60</Pattern>
			<Pattern>60</Pattern>
		</ScanPatternCustomPool>
	</PRML>
	<!-- SECL -->
	<SECL LightingFormat="drct" DisableAtLstg3="true" >
		<PresetPatterns>
			<Lstg1 Enabled="true" Pattern="20" />
			<Lstg2 Enabled="true" Pattern="20" />
			<Lstg3 Enabled="true" Pattern="20" />
		</PresetPatterns>
		<ForcedPatterns>
			<MainHorn Enabled="false" Pattern="20" />
			<SrnTone1 Enabled="false" Pattern="20" />
			<SrnTone2 Enabled="false" Pattern="20" />
			<SrnTone3 Enabled="false" Pattern="20" />
			<SrnTone4 Enabled="false" Pattern="20" />
			<PanicMde Enabled="false" Pattern="20" />
			<OutOfVeh Enabled="true" Pattern="20" />
		</ForcedPatterns>
		<ScanPatternCustomPool Enabled="true" Sequential="true" >
			<Pattern>20</Pattern>
			<Pattern>20</Pattern>
			<Pattern>20</Pattern>
		</ScanPatternCustomPool>
	</SECL>
</vcfroot>
